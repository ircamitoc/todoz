  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "jsx": "react-native",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "typings/*.d.ts"
  ],
